{
  "devServer": {
    "host": "0.0.0.0",
    "port": 3000
  },
  "entry": {
    "index": "./src/index.tsx"
  },
  "hash": true,
  "hmr": {},
  "clean": true,
  "resolve": {
    "alias": [
      [
        "@",
        "./src"
      ]
    ],
    "extensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ]
  },
  "stats": {
    "modules": true
  },
  "plugins": [
    "./plugins.config.js"
  ],
  "minify": false
}