{
  "devServer": {
    "host": "0.0.0.0",
    "port": 3000
  },
  "entry": {
    "index": "./src/index.tsx",
    "dashboard": "./src/app/dashboard/page.tsx",
    "login": "./src/app/login/page.tsx"
  },
  "hash": true,
  "hmr": {},
  "clean": true,
  "resolve": {
    "alias": [
      ["@", "./src"]
    ],
    "extensions": ["js", "jsx", "ts", "tsx"]
  },
  "stats": {
    "modules": true
  },
  "plugins": [
    "./plugins.config.js.bak"
  ],
  "minify": false
}