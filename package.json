    "@types/node": "^22.10.5",
    "@types/react": "^19.0.4",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/parser": "^8.19.1",
    "@umijs/mako": "^0.11.1",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "daisyui": "^4.12.23",
    "esbuild": "^0.24.2",
    "esbuild-register": "^3.6.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.14.0",
